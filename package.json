{
  "name": "turo-app",
  "version": "0.0.1",
  "description": "Javascript backend for turo calculator apps",
  "main": "./lib/app/turo-application.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "istanbul cover ./test/index.js && istanbul report html && open coverage/index.html"
  },
  "browserify": {
    "transform": [
      "brfs",
      "overidify"
    ]
  },
  "aliasify": "config/extensions.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/jhugman/turo-app.git"
  },
  "keywords": [
    "turo",
    "app",
    "cross-platform"
  ],
  "author": "James Hugman",
  "license": "All rights reserved",
  "bugs": {
    "url": "https://github.com/jhugman/turo-app/issues"
  },
  "homepage": "https://github.com/jhugman/turo-app",
  "dependencies": {
    "turo": "jhugman/turo",
    "underscore": "~1.5.2",
    "overidify": "jhugman/overidify"
  },
  "devDependencies": {
    "browserify": "~3.14.1",
    "grunt-browserify": "~1.3.0",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-contrib-connect": "~0.5.0",
    "grunt-contrib-copy": "~0.4.1",
    "tap": "~0.4.12",
    "grunt-contrib-jshint": "~0.7.1",
    "grunt-node-tap": "0.1.44",
    "lodash": "~2.4.1",
    "brfs": "0.0.8",
    "grunt-manifest": "~0.4.0",
    "grunt-contrib-clean": "~0.5.0",
    "build-facets": "jhugman/build-facets",
    "kirin-idl": "jhugman/kirin-idl",
    "macro-aliasify": "jhugman/macro-aliasify",
    "chai": "~1.10.0",
    "mocha": "~2.0.1"
  },
  "optionalDependencies": {
    "kirin": "jhugman/kirin"
  }
}
